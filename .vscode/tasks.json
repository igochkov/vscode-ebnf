{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Compile extension",
      "type": "shell",
      "command": "npm",
      "args": [
        "run",
        "build"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Package Extension",
      "type": "shell",
      "command": "npm",
      "args": [
        "run",
        "package"
      ],
      "group": {
        "kind": "build",
        "isDefault": false
      }
    }
  ]
}